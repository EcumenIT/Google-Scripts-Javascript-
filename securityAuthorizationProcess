  var kioskAccessType = e.values[16];
  
  // // // // For PCC at Kiosks:
  
        var kioskPCCAccessType = e.values[17];
  
  // // // // For ServiceMinder at Kiosks:
   
        var kioskSMAccessType = e.values[18];
  
