///////////// ***************

RSL CODE - Input separately as script to the RSL Google Sheet

function RSLonFormSubmit(e) {
  
  // Variable declarations - pulling from sheet/form
  
  var saApproverEmail = 'saforms@ecumen.org'; // will change to saforms@ecumen.org
  var time = e.values[0];
  var submitterEmail = e.values[1];
  var hiringManagerEmail = e.values[2];
  var employeeFirstLastName = e.values[3];
  var employeeMiddleInitial = e.values[4];
  var employeeRNumber = e.values[5];
  var employeePosition = e.values[6];
  var employeeLocation = e.values[7];
  var notes = e.values[8];
  var natureOfRequest = e.values[9];
  var emailAccessNeeded = e.values[10];
  var serviceMinderAccessNeeded = e.values[11];
  var serviceMinderAccessType = e.values[12];
  var certifyHipaaCompliance = e.values[13];
  
  // Variable declarations for email
  
  var subject = "New Security Access Request for RSL Received"
  var hiringManagerMessage = "Hello,<br><br>Ecumen IT has received a new request related to security access for an employee you are either hiring, terminating, or that is transferring positions. Please see the details below related to this request.<br><br><table border='1px',width='100%'><tr><td><b>Time of Request:</b> " + time + "</td></tr><tr><td><b>Email Address of Form Submitter:</b> " + submitterEmail + "</td></tr><tr><td><b>Nature of Request:</b> " + natureOfRequest + "</td></tr><tr><td><b>Name:</b> " + employeeFirstLastName + "</td></tr><tr><td><b>Employee R Number:</b> " + employeeRNumber + "</td></tr><tr><td><b>Position:</b> " + employeePosition + "</td></tr><tr><td><b>Location:</b> " + employeeLocation + "</td></tr><tr><td><b>Additional Notes:</b> " + notes + "</td></tr><tr><td><b>Email Access Needed:</b> " + emailAccessNeeded + "</td></tr><tr><td><b>ServiceMinder Access Needed:</b> " + serviceMinderAccessNeeded + "</td></tr><tr><td><b>ServiceMinder Access Requested, if Any:</b> " + serviceMinderAccessType + "</td></tr></table><br><br>If you believe you've received this message in error or if any of the details above are incorrect, please contact saforms@ecumen.org to notify our team.<br><br>Thank you,<br><br><b>The Ecumen Business Transformation and Technology Team</b><br><br>";
  var saApproverMessage = "Hello,<br><br>A new request has been submitted related to security access for an RSL employee that is either being hired, terminated, or is transferring positions. Please see the details below related to this request.<br><br><table border='1px',width='100%'><tr><td><b>Time of Request:</b> " + time + "</td></tr><tr><td><b>Email Address of Form Submitter:</b> " + submitterEmail + "</td></tr><tr><td><b>Hiring Manager Email Address:</b> " + hiringManagerEmail + "</td></tr><tr><td><b>Nature of Request:</b> " + natureOfRequest + "</td></tr><tr><td><b>Name:</b> " + employeeFirstLastName + "</td></tr><tr><td><b>Employee R Number:</b> " + employeeRNumber + "</td></tr><tr><td><b>Position:</b> " + employeePosition + "</td></tr><tr><td><b>Location:</b> " + employeeLocation + "</td></tr><tr><td><b>Additional Notes:</b> " + notes + "</td></tr><tr><td><b>Email Access Needed:</b> " + emailAccessNeeded + "</td></tr><tr><td><b>ServiceMinder Access Needed:</b> " + serviceMinderAccessNeeded + "</td></tr><tr><td><b>ServiceMinder Access Requested, if Any:</b> " + serviceMinderAccessType + "</td></tr></table><br><br>If you believe you've received this message in error or if any of the information above is incorrect, please contact " + submitterEmail + " to ask for more details.<br><br>";
  
  // Variable declaration for image
  
  var ecumenLogoImageUrl = "http://webnet.ecumen.org/images/EcumenLogo_NoTag.png";
  
  // Image to blob
  
  var ecumenLogoBlob = UrlFetchApp.fetch(ecumenLogoImageUrl).getBlob().setName("ecumenLogoBlob");
  
  
  // Prepend embedded image tags for email
  
  hiringManagerMessage = hiringManagerMessage + "<img src='cid:ecumenLogoBlob' style='float:center;'/>";
  
  // Send email to hiring manager
  
  MailApp.sendEmail (hiringManagerEmail, subject, "", {htmlBody: hiringManagerMessage, inlineImages: {ecumenLogoBlob: ecumenLogoBlob}});
  
  // Send email to Ecumen SA approver
  
  MailApp.sendEmail (saApproverEmail, subject, "", {htmlBody: saApproverMessage, inlineImages: {ecumenLogoBlob: ecumenLogoBlob}});
                
}

